<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Widget.App.Button" parent="Widget.MaterialComponents.Button">
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="textAllCaps">false</item>
    </style>


    <style name="Widget.App.BottomNavBar" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="labelVisibilityMode">unlabeled</item>
        <item name="itemRippleColor">@color/ripple_effect</item>
    </style>


    <style name="ShapeAppearanceOverlay" parent="" />

    <style name="ShapeAppearanceOverlay.App.Circle" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.Rounded" parent="">
        <item name="cornerSize">5dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.OtherMessage" parent="">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.MyMessage" parent="">
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
    </style>


    <style name="ShapeAppearance.App.BottomSheetDialog" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">5dp</item>
        <item name="cornerSizeTopRight">5dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>


    <style name="RoundedIconContainer5">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
    </style>

    <style name="SectionLabelTextStyle">
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

</resources>